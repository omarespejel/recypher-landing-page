1038:           const t = time * 0.0002;
1039:           colors.forEach((color, index) => {
1040:             const x = (noise3D(index, t, 0) * 0.5 + 0.5) * gradientCanvas.width;
1041:             const y = (noise3D(0, index, t) * 0.5 + 0.5) * gradientCanvas.height;
1042:             const radius = gradientCanvas.width * 0.6;
