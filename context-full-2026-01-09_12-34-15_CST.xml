 909:           const chevron = item.querySelector('.faq-chevron');
 910:           if (!toggle || !answer) return;
 911: 
 912:           toggle.addEventListener('click', () => {
 913:             const isOpen = item.getAttribute('data-state') === 'open';
 914:             faqItems.forEach((otherItem) => {
 915:               if (otherItem === item) return;
 916:               otherItem.setAttribute('data-state', 'closed');
 917:               const otherToggle = otherItem.querySelector('.faq-toggle');
 918:               const otherAnswer = otherItem.querySelector('.faq-answer');
 919:               const otherChevron = otherItem.querySelector('.faq-chevron');
 920:               if (otherToggle) otherToggle.setAttribute('aria-expanded', 'false');
 921:               if (otherAnswer) otherAnswer.style.maxHeight = '0px';
 922:               if (otherChevron) otherChevron.classList.remove('rotate-180');
 923:             });
 924: 
 925:             item.setAttribute('data-state', isOpen ? 'closed' : 'open');
 926:             toggle.setAttribute('aria-expanded', isOpen ? 'false' : 'true');
 927:             answer.style.maxHeight = isOpen ? '0px' : `${answer.scrollHeight}px`;
 928:             if (chevron) chevron.classList.toggle('rotate-180', !isOpen);
 929:           });
 930:         });
 931:       })();
