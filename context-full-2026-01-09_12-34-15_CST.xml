<file path="src/components/icons/LucideIcon.astro">
 1: ---
 2: import lucideStatic from 'lucide-static';
 3: 
 4: const { name, class: className = '', strokeWidth = 1.5 } = Astro.props;
 5: // Convert kebab-case to PascalCase (e.g., "arrow-right" -> "ArrowRight")
 6: const pascalName = name
 7:   .split('-')
 8:   .map(word => word.charAt(0).toUpperCase() + word.slice(1))
 9:   .join('');
10: const svg = lucideStatic[pascalName];
11: 
12: if (!svg) {
13:   throw new Error(`Unknown Lucide icon: ${name} (tried: ${pascalName})`);
14: }
15: 
16: let html = svg;
17: if (className) {
18:   html = html.replace('<svg ', `<svg class="${className}" `);
19: }
20: 
21: if (html.includes('stroke-width=')) {
22:   html = html.replace(/stroke-width="[^"]*"/, `stroke-width="${strokeWidth}"`);
23: } else {
24:   html = html.replace('<svg ', `<svg stroke-width="${strokeWidth}" `);
25: }
26: ---
27: <Fragment set:html={html} />
</file>

