1185:       const gradientCanvas = document.getElementById('gradient-canvas');
1186:       if (gradientCanvas && !prefersReducedMotion && !isNeo) {
1187:         const ctx = gradientCanvas.getContext('2d');
1188:         const noise3D = createNoise3D();
1189: 
1190:         const resizeCanvas = () => {
1191:           gradientCanvas.width = gradientCanvas.offsetWidth * window.devicePixelRatio;
1192:           gradientCanvas.height = gradientCanvas.offsetHeight * window.devicePixelRatio;
1193:         };
1194: 
1195:         const colors = [
1196:           'rgba(91, 126, 207, 0.35)',
1197:           'rgba(139, 92, 246, 0.25)',
1198:           'rgba(59, 130, 246, 0.2)',
1199:           'rgba(249, 115, 22, 0.15)',
1200:         ];
1201: 
1202:         const render = (time) => {
