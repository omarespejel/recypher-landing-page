1014:     <script define:vars={{ countdownLabels }} is:inline>
1015:       (() => {
1016:         const beginDate = new Date('2026-02-01T10:00:00-05:00').getTime();
1017:         const endDate = new Date('2026-02-28T23:59:59-05:00').getTime();
1018:         let countdownIntervalId;
1019: 
1020:         function updateCountdown() {
1021:           const now = Date.now();
1022:           
1023:           // Update "Submissions begin" countdown
1024:           let beginDistance = beginDate - now;
1025:           if (beginDistance < 0) {
1026:             beginDistance = 0;
1027:           }
1028:           
1029:           const beginDays = Math.floor(beginDistance / (1000 * 60 * 60 * 24));
1030:           const beginHours = Math.floor((beginDistance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
1031:           const beginMins = Math.floor((beginDistance % (1000 * 60 * 60)) / (1000 * 60));
1032:           const beginSecs = Math.floor((beginDistance % (1000 * 60)) / 1000);
1033: 
1034:           const beginDaysEl = document.getElementById('begin-days');
1035:           if (beginDaysEl) {
1036:             beginDaysEl.textContent = String(beginDays).padStart(2, '0');
1037:             document.getElementById('begin-hours').textContent = String(beginHours).padStart(2, '0');
1038:             document.getElementById('begin-mins').textContent = String(beginMins).padStart(2, '0');
1039:             document.getElementById('begin-secs').textContent = String(beginSecs).padStart(2, '0');
1040:           }
1041: 
1042:           // Update "Submissions end" countdown
1043:           let endDistance = endDate - now;
1044:           if (endDistance < 0) {
1045:             endDistance = 0;
1046:             if (countdownIntervalId) clearInterval(countdownIntervalId);
1047:           }
1048:           
1049:           const endDays = Math.floor(endDistance / (1000 * 60 * 60 * 24));
1050:           const endHours = Math.floor((endDistance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
1051:           const endMins = Math.floor((endDistance % (1000 * 60 * 60)) / (1000 * 60));
1052:           const endSecs = Math.floor((endDistance % (1000 * 60)) / 1000);
1053: 
1054:           const endDaysEl = document.getElementById('end-days');
1055:           if (endDaysEl) {
1056:             endDaysEl.textContent = String(endDays).padStart(2, '0');
1057:             document.getElementById('end-hours').textContent = String(endHours).padStart(2, '0');
1058:             document.getElementById('end-mins').textContent = String(endMins).padStart(2, '0');
1059:             document.getElementById('end-secs').textContent = String(endSecs).padStart(2, '0');
1060:           }
1061:         }
1062: 
1063:         updateCountdown();
1064:         countdownIntervalId = setInterval(updateCountdown, 1000);
1065:       })();
1066:     </script>
