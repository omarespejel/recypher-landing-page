1456:       // FAQ ACCORDION
1457:       // ==========================================
1458:       const faqItems = document.querySelectorAll('.faq-item');
1459:       faqItems.forEach((item) => {
1460:         const toggle = item.querySelector('.faq-toggle');
1461:         const answer = item.querySelector('.faq-answer');
1462:         const chevron = item.querySelector('.faq-chevron');
1463:         
1464:         if (!toggle || !answer) return;
1465:         
1466:         toggle.addEventListener('click', () => {
1467:           const isOpen = item.getAttribute('data-state') === 'open';
1468:           
1469:           faqItems.forEach((otherItem) => {
1470:             if (otherItem === item) return;
1471:             otherItem.setAttribute('data-state', 'closed');
1472:             const otherToggle = otherItem.querySelector('.faq-toggle');
1473:             const otherAnswer = otherItem.querySelector('.faq-answer');
1474:             const otherChevron = otherItem.querySelector('.faq-chevron');
1475:             if (otherToggle) otherToggle.setAttribute('aria-expanded', 'false');
1476:             if (otherAnswer) otherAnswer.style.maxHeight = '0px';
1477:             if (otherChevron) otherChevron.classList.remove('rotate-180');
1478:           });
1479:           
1480:           item.setAttribute('data-state', isOpen ? 'closed' : 'open');
1481:           toggle.setAttribute('aria-expanded', isOpen ? 'false' : 'true');
1482:           answer.style.maxHeight = isOpen ? '0px' : `${answer.scrollHeight}px`;
1483:           if (chevron) chevron.classList.toggle('rotate-180', !isOpen);
1484:         });
1485:       });
1486:       } // End window check
