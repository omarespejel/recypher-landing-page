 856:     <script define:vars={{ countdownLabels }} is:inline>
 857:       (() => {
 858:         const targetDate = new Date('2026-02-01T10:00:00-05:00').getTime();
 859:         const endDate = new Date('2026-02-28T23:59:59-05:00').getTime();
 860:         let countdownIntervalId;
 861: 
 862:         function updateCountdown() {
 863:           const daysEl = document.getElementById('days');
 864:           if (!daysEl) return;
 865: 
 866:           const now = Date.now();
 867:           let distance = targetDate - now;
 868:           let label = countdownLabels.starts;
 869: 
 870:           if (distance < 0) {
 871:             distance = endDate - now;
 872:             label = countdownLabels.ends;
 873:             const labelEl = document.querySelector('#countdown')?.previousElementSibling;
 874:             if (labelEl) labelEl.textContent = label;
 875:           }
 876: 
 877:           if (distance < 0) {
 878:             daysEl.textContent = '00';
 879:             document.getElementById('hours').textContent = '00';
 880:             document.getElementById('mins').textContent = '00';
 881:             document.getElementById('secs').textContent = '00';
 882:             if (countdownIntervalId) clearInterval(countdownIntervalId);
 883:             return;
 884:           }
 885: 
 886:           const days = Math.floor(distance / (1000 * 60 * 60 * 24));
 887:           const hours = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
 888:           const mins = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
 889:           const secs = Math.floor((distance % (1000 * 60)) / 1000);
 890: 
 891:           daysEl.textContent = String(days).padStart(2, '0');
 892:           document.getElementById('hours').textContent = String(hours).padStart(2, '0');
 893:           document.getElementById('mins').textContent = String(mins).padStart(2, '0');
 894:           document.getElementById('secs').textContent = String(secs).padStart(2, '0');
 895:         }
 896: 
 897:         updateCountdown();
 898:         countdownIntervalId = setInterval(updateCountdown, 1000);
 899:       })();
 900:     </script>
