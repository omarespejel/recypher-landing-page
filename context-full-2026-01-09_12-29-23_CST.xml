<file path="Makefile">
 1: .PHONY: help context clean
 2: 
 3: help:
 4: 	@echo "Context generation commands:"
 5: 	@echo "  make context         - Generate full project context using repomix"
 6: 	@echo ""
 7: 	@echo "Cleanup commands:"
 8: 	@echo "  make clean           - Remove generated context files"
 9: 
10: context:
11: 	@echo "Generating full project context..."
12: 	@DATE=$$(date '+%Y-%m-%d_%H-%M-%S_%Z'); \
13: 	OUTPUT_FILE="context-full-$${DATE}.xml"; \
14: 	cp repomix.config.json repomix.config.json.bak && \
15: 	jq ".output.filePath = \"$$OUTPUT_FILE\"" repomix.config.json > repomix.config.json.tmp && \
16: 	mv repomix.config.json.tmp repomix.config.json && \
17: 	(repomix --config repomix.config.json || (mv repomix.config.json.bak repomix.config.json && exit 1)) && \
18: 	jq ".output.filePath = \"context-full.xml\"" repomix.config.json > repomix.config.json.tmp && \
19: 	mv repomix.config.json.tmp repomix.config.json && \
20: 	rm -f repomix.config.json.bak && \
21: 	echo "âœ… Context written to $$OUTPUT_FILE"
22: 
23: clean:
24: 	@echo "Cleaning generated context files..."
25: 	rm -f context*.xml
26: 	rm -f context-*-*.xml
27: 	rm -f repomix.config.json.bak
28: 	rm -f repomix.config.json.tmp
</file>

